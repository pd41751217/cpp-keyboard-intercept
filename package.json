{
  "name": "cpp-demo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "launch": "nx run-many --parallel --target=serve --projects=app-electron,renderer-window,overlay-window",
    "build:electron": "npx electron-builder -p never -c apps/app-electron/electron-builder.json",
    "postinstall": "electron-builder install-app-deps",
    "start": "nx run-many --parallel --target=serve --projects=app-electron,renderer-window,overlay-window",
    "--- App ---": "",
    "app-electron:serve": "nx serve app-electron",
    "--- Renderer Window ---": "",
    "renderer-window:serve": "nx serve renderer-window",
    "--- Overlay Window ---": "",
    "overlay-window:serve": "nx serve overlay-window",
    "--- Natives ---": "",
    "natives:install": "npm run natives:build && npm run natives:addon:build && npm run natives:copy  ",
    "natives:build": "cd ./apps/app-electron/src/native/overlay-api/win-natives/tasks/ && build.bat && copy.bat",
    "natives:addon:build": "cd ./apps/app-electron/src/native/overlay-api/node-natives/ && cmake-js -G \"Visual Studio 17 2022\" -A x64 -O build_x64 && cd ./tasks/ && copy.bat",
    "natives:addon:copy": "cd ./apps/app-electron/src/native/overlay-api/node-natives/tasks/ && copy.bat"
  },
